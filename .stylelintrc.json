{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recommended-vue"
  ],
  "rules": {
    "selector-id-pattern": null,
    "custom-property-pattern": null,
    "selector-class-pattern": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "define-mixin",
          "mixin",
          "value"
        ]
      }
    ]
  }
}
